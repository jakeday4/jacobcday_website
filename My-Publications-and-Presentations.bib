@unpublished{brauer_jonathan_r_people_nodate,
	title = {Do {People} {Acclimate} to {Chronic} {Stress}? {Community} {Differences} in {Within}-{Person} {Associations} {Between} {Subjective} {Stress}, {Crime}, and {Depressive} {Symptoms}},
	abstract = {The current study then applies multilevel hybrid or “between-within” models (Allison, 2009; Curran and Bauer, 2011) to two waves of panel survey data from a random sample of adults in Dhaka, Bangladesh to assesses whether correlative patterns are consistent with stress process theories. Between-person (cross-sectional) portions of these models, which replicate patterns frequently documented in other social settings in a rarely studied cultural context, assess: (1) whether residents of disadvantaged (e.g., low SES urban) communities report more stress, and (2) whether individuals who experience more stress also report more depressive symptoms and higher criminality (i.e., criminal intent and behavior). Within-person (longitudinal) portions of these models, which eliminate time-stable sources of confounding in estimating stress-outcome associations, assess: (3) whether changes in stress levels across a two-year period are positively correlated as expected with changes in depression and criminality, and (4) whether these change correlations vary systematically across social locations. Regarding the latter, we explore whether positive stress-outcome change correlations are strongest (consistent with stress amplification hypothesis) or weakest (consistent with stress acclimation hypothesis) among residents of low-SES urban communities where chronic stress exposure is most common.},
	author = {{Brauer, Jonathan R.} and {Day, Jacob C.} and {Kotlaja, Marijana M.}},
}

