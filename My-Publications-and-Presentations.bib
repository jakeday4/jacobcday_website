@unpublished{farrell_bullying_nodate,
	address = {San Francisco, CA},
	title = {Bullying {Behavior} in {Context}: {A} {Macro}-{Level} {Analysis} of {School} {Bullying}},
	abstract = {The purpose of this study is to examine school-level factors which influence bullying behavior guided by theoretical ideas and research findings found in the school climate and criminological literature on bullying and macro-level general strain theory. Using nationally representative sampling frame, data were collected from 314 schools across the United States in 2009-2010. Results of OLS regression analyses show support for predictions from both the school climate literature and macro-level general strain theory. Specifically, racial heterogeneity, high amounts of pressure from schoolwork, poor peer-to-peer relationships, school-level negative emotions (e.g. proportion of students feeling low and feeling frustrated with their appearance) are associated with school-level traditional bullying victimization. Schools in which students spent more time on the computer were more likely to have instances of cyberbullying victimization. Also, negative family relationships was found to predict cyberbullying victimization, as well as the two negative emotions of feeling low and frustration. The study concludes with a discussion of limitations and directions for future research and provides critical suggestions regarding a popular bullying prevention program.},
	language = {en},
	author = {Farrell, Mitchel Allen and Day, Jacob C.},
}

